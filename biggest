	<body>
		<div>
			<header>
				<h1>比较三个数中最大的值</h1>
			</header>
			 
		</div>
		<script>
		    
			var a = parseInt(window.prompt("input"));
			var b = parseInt(window.prompt("input"));
			var c = parseInt(window.prompt("input"));
			
			if(a >= b){
				if(a >= c){
					document.write("这三个数中最大的是 " + a );	
				}
				else{
					document.write("这三个数中最大的是 " + c );	
				}					
			}
			else{
				if(b >= c){
					document.write("这三个数中最大的是 : " + b );	
				}
				else{
					document.write("这三个数中最大的是 " + c );	
				}							 
			}		
		</script>
	</body>
